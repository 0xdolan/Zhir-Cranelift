{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Listener": {
    "IntervalSeconds": 3.5
  },
  "Worker": {
    "WorkerCount": 1,
    "ParallelPagesCount": 4,
    "EnforceBalance": true
  },
  "AllowedHosts": "*",
  "Python": {
    "Path": "python3"
  }
}
